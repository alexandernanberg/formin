{
  "name": "formin",
  "version": "0.0.1",
  "description": "Primitive for building forms in React.",
  "license": "MIT",
  "author": "Alexander Nanberg  <alex.nanberg@hotmail.com> (https://alexandernanberg.com)",
  "main": "dist/formin.cjs.js",
  "module": "dist/formin.esm.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "kcd-scripts lint",
    "test": "kcd-scripts test",
    "test:cover": "kcd-scripts test --coverage",
    "build": "kcd-scripts build --bundle",
    "validate": "kcd-scripts validate lint,test:cover",
    "storybook": "start-storybook -p 6006 -c stories",
    "storybook:build": "build-storybook -c stories",
    "storybook:serve": "serve ./storybook-static -p 6006",
    "storybook:install": "cd stories && npm install",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexandernanberg/formin.git"
  },
  "bugs": {
    "url": "https://github.com/alexandernanberg/formin/issues"
  },
  "homepage": "https://github.com/alexandernanberg/formin#readme",
  "keywords": [
    "react",
    "primitive",
    "form"
  ],
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@storybook/react": "^4.0.0-alpha.16",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.3.0",
    "eslint-config-alexandernanberg": "^1.3.2",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "kcd-scripts": "^0.42.1",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-testing-library": "^5.0.0",
    "serve": "^10.0.0"
  },
  "lint-staged": {
    "**/*.+(js|json|less|css|ts|tsx|md)": [
      "kcd-scripts lint",
      "git add"
    ]
  }
}
