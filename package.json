{
  "name": "formin",
  "version": "0.0.0-semantically-released",
  "description": "Primitive for building forms in React.",
  "license": "MIT",
  "author": "Alexander Nanberg  <alex.nanberg@hotmail.com> (https://alexandernanberg.com)",
  "main": "dist/formin.cjs.js",
  "module": "dist/formin.esm.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "kcd-scripts lint",
    "test": "kcd-scripts test",
    "test:cover": "kcd-scripts test --coverage",
    "build": "kcd-scripts build --bundle",
    "docz": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "serve .docz/dist -p 6006",
    "validate": "kcd-scripts validate lint,build,test:cover",
    "setup": "yarn && yarn validate",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexandernanberg/formin.git"
  },
  "bugs": {
    "url": "https://github.com/alexandernanberg/formin/issues"
  },
  "homepage": "https://github.com/alexandernanberg/formin#readme",
  "keywords": [
    "react",
    "primitive",
    "form"
  ],
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "docz": "^0.11.0",
    "eslint": "^5.4.0",
    "eslint-config-alexandernanberg": "^1.4.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "kcd-scripts": "^0.44.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-testing-library": "^5.0.0",
    "serve": "^10.0.0"
  },
  "lint-staged": {
    "**/*.+(js|json|css|md)": [
      "kcd-scripts lint",
      "git add"
    ]
  }
}
